        app:layout_constraintStart_toEndOf="@+id/mondayStartCoreTimeLabel"
        app:layout_constraintTop_toBottomOf="@+id/coreTimeCustomLabel" />

    <CheckBox
        android:id="@+id/isMondayCoreDayBox"
