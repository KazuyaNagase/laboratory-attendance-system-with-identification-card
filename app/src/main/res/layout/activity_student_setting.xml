        android:layout_height="14dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="CheckBox"
        app:layout_constraintStart_toEndOf="@+id/mondayEndCoreTimeLabel"
        app:layout_constraintTop_toBottomOf="@+id/coreTimeCustomLabel" />

    <TextView
        android:id="@+id/tuesdayLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mondayLabel" />

    <TextView
        android:id="@+id/wednesdayLabel"
